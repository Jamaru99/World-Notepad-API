{
    "env": {
        "commonjs": true,
        "node": true,
        "mocha": true
    },
    "extends": "strongloop",
    "parserOptions": { 
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "max-len": ["error", { "code": 130 }]
    }
}
